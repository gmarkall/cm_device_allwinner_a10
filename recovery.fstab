# mount point	fstype		device

/boot		ext4		/dev/block/nandb
/cache		ext4		/dev/block/nandg
/data		ext4		/dev/block/nandd
/emmc		vfat		/dev/block/nandh
/recovery	ext4		/dev/block/nandf
/sdcard		vfat		/dev/block/mmcblk0p1	/dev/block/mmcblk0
/sd-ext		auto		/dev/block/mmcblk0p2
/system		ext4		/dev/block/nandc
